#include<stdio.h>
int main()
{
	 int i,n,sum=0,x;
	 printf("enter the size of an array : ");
	 scanf("%d",&n);
	 int arr[n-1];
	 printf("insert the values into array: \n");
	 for(i=0;i<n-1;i++){
	     scanf("%d",&arr[i]);
	     sum=sum+arr[i];
	 }
	 x= (n*(n+1))/2 - sum;
	 printf("missing number is = %d", x);
	 return(0);
}
